        </th>
    </tr>
</thead>
<tbody id="@Model.ContentViewModel.PagedListViewModel.TargetTableBody" class="text-nowrap">
    @foreach (var item in Model.ContentViewModel.PagedListViewModel.PagedList)
    {
        <tr>
            @* <td>@Html.DisplayFor(modelItem => item.GroupArea.Name)</td>*@
            <td>@Html.DisplayFor(modelItem => item.EmployeeNumber)</td>
            @*<td>@Html.DisplayFor(modelItem => item.FirstName)</td>
                <td>@Html.DisplayFor(modelItem => item.LastName)</td>
                <td>@Html.DisplayFor(modelItem => item.MiddleName)</td>*@
            <td>@Html.DisplayFor(modelItem => item.DisplayName)</td>
            @*<td>@Html.DisplayFor(modelItem => item.BirthDate)</td>*@
            <td>@Html.DisplayFor(modelItem => item.Gender)</td>
            @*<td>@Html.DisplayFor(modelItem => item.CivilStatus)</td>
                <td>@Html.DisplayFor(modelItem => item.SssNumber)</td>
                <td>@Html.DisplayFor(modelItem => item.TinNumber)</td>
                <td>@Html.DisplayFor(modelItem => item.PassportNumber)</td>*@
            <td>@Html.DisplayFor(modelItem => item.DepartmentName)</td>
            @*<td>@Html.DisplayFor(modelItem => item.MembershipDate)</td>
                <td>@Html.DisplayFor(modelItem => item.HiredDate)</td>
                <td>@Html.DisplayFor(modelItem => item.EmploymentEndDate)</td>*@
            <td>
                @Html.Partial("_EditDetailsDelete", Model.ContentViewModel.GenerateEditDetailsDeleteLinks(Html, item.DisplayName, item.Id, Model.ContentViewModel, Model.ReturnUrl, " | "))
            </td>
        </tr>
    }

</tbody>